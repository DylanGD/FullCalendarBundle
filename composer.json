{
    "name": "dylangd/full-calendar-bundle",
    "description": "Symfony4 integration with the library FullCalendar.js",
    "type": "symfony-bundle",
    "keywords": ["calendar", "fullcalendar", "jquery calendar", "symfony4 calendar", "symfony4 bundle"],
    "homepage": "https://github.com/DylanGD/FullCalendarBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "DylanGD",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",

        "symfony/framework-bundle": "^4.0.3"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "DylanGD\\FullCalendarBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\DylanGD\\FullCalendarBundle\\": "spec"
        }
    }
}
